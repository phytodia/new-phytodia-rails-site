<h1>Faqs#new</h1>
<p>Find me in app/views/faqs/new.html.erb</p>
<%= simple_form_for @faq, url: {action: "create"} do |f|%>
  <%= f.input :question %>
  <%= f.input :answer %>
  <%= f.input :lang, collection:["fr","en","de","es"], selected:0 %>
  <%= f.input :categories,as: :check_boxes, collection: Faq::CATEGORIES %>
  <%= f.submit "Sauvegarder"%>
<% end %>
